separator_block_width=14

[time]
command=date '+%d.%m.%Y %H:%M'
interval=5
color=#50C878
align=center

[disk-root]
label=ROOT
command=/usr/lib/i3blocks/disk /
interval=30
color=#1793D1

[disk-home]
label=HOME
command=/usr/lib/i3blocks/disk $HOME
interval=30
color=#859900

[ssid]
command=echo "$(iw dev wlan0 link | grep SSID | cut -d " " -f 2-)"
color=#d33682
interval=5

[volume]
command=/usr/lib/i3blocks/volume
interval=2
signal=10
color=#d70a53

